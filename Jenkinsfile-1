pipeline {
  agent {
    node {
      label 'GUI'
    }
  }
  stages{
  
	stage('Checkout code from SCM') {
			steps{
				cleanWs()
				
				checkout([$class: 'GitSCM',
					branches: [
						[name: "main"]
					],
					doGenerateSubmoduleConfigurations: false,
					extensions: [],
					gitTool: 'Default',
					submoduleCfg: [],
					userRemoteConfigs: [
						[credentialsId: 'GitAuth',
							url: 'https://github.com/Narendrakaduru/SKRcollege.git'
						
						]
					]
				])
			}
		}
	
  }
}
